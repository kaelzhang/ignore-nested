    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/ignore-nested/issues"
  },
  "devDependencies": {
    "ava": ">= 1.0.0-beta.6",
    "codecov": "^3.0.2",
    "eslint": "^5.1.0",
    "eslint-config-ostai": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "lodash.foreach": "^4.5.0",
    "mkdirp": "^0.5.1",
    "nyc": "^12.0.2",
    "pre-suf": "^1.1.0",
    "rimraf": "^2.6.2",
    "spawn-sync": "^2.0.0",
    "tmp": "0.0.33"
  },
  "dependencies": {
    "ignore": "^4.0.3"
